{
  "name": "@form8ion/remark-update-legacy-badge-markers",
  "description": "remark plugin to update legacy badge markers to the modern zone convention",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "form8ion/remark-update-legacy-badge-markers",
  "bugs": "https://github.com/form8ion/remark-update-legacy-badge-markers/issues",
  "homepage": "https://npm.im/@form8ion/remark-update-legacy-badge-markers",
  "runkitExampleFilename": "./example.js",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./lib",
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:gherkin": "gherkin-lint",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "DEBUG=any vitest run",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "pretest:integration:base": "npm run build:js",
    "test:integration:base": "cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "generate:md": "remark . --output",
    "pregenerate:md": "run-s build",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "files": [
    "example.js",
    "lib/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "unist-util-modify-children": "^2.0.0"
  },
  "devDependencies": {
    "@babel/register": "7.22.5",
    "@cucumber/cucumber": "9.3.0",
    "@form8ion/babel-preset": "1.6.107",
    "@form8ion/commitlint-config": "1.0.53",
    "@form8ion/eslint-config": "5.0.36",
    "@form8ion/eslint-config-cucumber": "1.4.1",
    "@form8ion/eslint-config-mocha": "2.0.3",
    "@form8ion/remark-lint-preset": "5.0.9",
    "@travi/any": "2.1.8",
    "ban-sensitive-files": "1.9.19",
    "c8": "8.0.1",
    "chai": "4.3.7",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.2",
    "husky": "8.0.3",
    "jest-when": "3.5.2",
    "lockfile-lint": "4.10.6",
    "mock-fs": "5.2.0",
    "npm-run-all": "4.1.5",
    "remark": "13.0.0",
    "remark-cli": "9.0.0",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1",
    "rimraf": "5.0.1",
    "rollup": "3.27.0",
    "rollup-plugin-auto-external": "2.0.0",
    "vitest": "0.33.0"
  }
}
